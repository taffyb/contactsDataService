{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "publisher": {
      "type": "string",
      "minLength": 1
    },
    "publishedDate": {
      "type": "string",
      "minLength": 1
    },
    "contributor": {
      "type": "string",
      "minLength": 1
    },
    "contributedDate": {
      "type": "string",
      "minLength": 1
    },
    "uri": {
      "type": "string",
      "minLength": 1
    },
    "topics": {
      "type": "array",
      "items": {
        "required": [],
        "properties": {}
      }
    },
    "keywords": {
      "type": "array",
      "items": {
        "required": [],
        "properties": {}
      }
    },
    "measures": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "min": {
            "type": "number"
          },
          "max": {
            "type": "number"
          },
          "values": {
            "type": "array"
          },
          "measuredBy": {
            "type": "String"
          },
          "isComparable": {
            "type": "Boolean"
          }
        }
      }
    },
    "linkedData": {
      "type": "array",
      "items": {
        "required": ["GUID"],
        "properties": {
		  "GUID":{
			"type": "string",
			"minLength": 1
		  }
		}
      }
    }
  },
  "required": [
    "name",
    "publisher",
    "publishedDate",
    "uri",
    "topics",
    "keywords",
    "measures"
  ]
}